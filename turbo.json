{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "CLERK_SECRET_KEY",
    "DATABASE_URL",
    "WEBHOOK_SECRET",
    "ADMIN_CLERK_ID",
    "REDIS_URL"
  ],
  "tasks": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "env": [
        "CLERK_SECRET_KEY",
        "DATABASE_URL",
        "WEBHOOK_SECRET",
        "ADMIN_CLERK_ID",
        "REDIS_URL",
        "CLERK_PUBLISHABLE_KEY"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "ui#build": {
      "cache": false,
      "persistent": true,
      "outputs": ["dist/**"],
      "env": [
        "CLERK_SECRET_KEY",
        "DATABASE_URL",
        "WEBHOOK_SECRET",
        "ADMIN_CLERK_ID",
        "REDIS_URL",
        "DIRECT_URL"
      ]
    },
    "gamex#build": {
      "persistent": true,
      "env": [
        "CLERK_SECRET_KEY",
        "DATABASE_URL",
        "WEBHOOK_SECRET",
        "ADMIN_CLERK_ID",
        "REDIS_URL",
        "DIRECT_URL"
      ]
    },
    "prettier": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": [".env", ".env.local", "NODE_ENV"],
  "ui": "tui"
}