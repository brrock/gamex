{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "inputs": [".env", ".env.local", "DATABASE_URL", "CLERK_SERCET_KEY", "WEBHOOK_SECRET", "REDIS_URL", "ADMIN_CLERK_ID"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "ui:build": {
      "cache": false,
      "persistent": true,
      "outputs": ["dist/**"]
    },
    "gamex#build": {  
      "inputs": [".env", ".env.local", "DATABASE_URL", "CLERK_SERCET_KEY", "WEBHOOK_SECRET", "REDIS_URL", "ADMIN_CLERK_ID"],
      "persistent": true 
    },
   "prettier": {
      "cache": false,
      "persistent": true
    }
  },
  "globalDependencies": [".env", ".env.local" ],

  "ui": "tui"

}
